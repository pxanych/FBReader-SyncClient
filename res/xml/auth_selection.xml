<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen 
xmlns:android="http://schemas.android.com/apk/res/android"
android:title="@string/auth"
android:key="@string/auth_key"
>    
	<Preference 
	android:key="@string/auth_our_acc_sign_in_key" 
	android:title="@string/auth_our_acc_sign_in"
	/>
	
	<Preference 
	android:key="@string/auth_our_acc_register_key" 
	android:title="@string/auth_our_acc_register"
	/>
	
	<PreferenceScreen
	android:title="@string/auth_sign_in">
		    
		    <com.android.settings.IconPreferenceScreen 
    		android:key="@string/auth_google_key" 
    		android:title="@string/auth_sign_in_google"
    		/>
	
			<com.android.settings.IconPreferenceScreen 
			android:key="@string/auth_facebook_key" 
			android:title="@string/auth_sign_in_facebook"
			/>
		
	</PreferenceScreen>
	
</PreferenceScreen>
